
public class Program {
	public static void main(String args[]) {
		int a =1;
		int b =2;
		int c =3;
		
		//A
		int A = (2*a + 3*b)*c;
		System.out.println(A);
		
		//B
		int B = (2*a % 6);
		System.out.println(B);
		
		//C
		int C = (-a * 4*b);
		System.out.println(C);
		
		//D
		int D = (a - 2*b*c);
		System.out.println(D);
		
	}
}
